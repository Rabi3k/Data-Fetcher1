RewriteEngine On


RewriteCond %{THE_REQUEST} %{ENV:ROOT_PATH}/public/([^\s?]*) [NC]
RewriteRule ^ %1 [L,NE,R=302]
RewriteRule ^api/(.*)/([0-9]+)-([0-9]+)$ public/index.php?q=$1&s=$2&e=$3 [L,QSA]
RewriteRule ^api/(.*)/([0-9]+)$ public/index.php?q=$1&id=$2 [L,QSA]
RewriteRule ^api/(.*)/$ public/index.php?q=$1 [L,QSA]
RewriteRule ^api/(.*)$ public/index.php?q=$1 [L,QSA]
RewriteRule ^api/$ public/index.php [L,QSA]
RewriteRule ^api$ public/index.php [L,QSA]

RewriteCond %{THE_REQUEST} %{ENV:ROOT_PATH}/dashboard/([^\s?]*) [NC]
RewriteRule ^ %1 [L,NE,R=302]
RewriteRule ^admin/(.*)/(.*)$ dashboard/index.php?page=$1&q=$2 [L,QSA]
RewriteRule ^admin/(.*)$ dashboard/index.php?page=$1 [L,QSA]
RewriteRule ^admin$ dashboard/index.php [L,QSA]

ErrorDocument 403 /error.php
#ErrorDocument 404 /data%20fetcher/error.php
ErrorDocument 404 /error.php
ErrorDocument 500 /error.php

